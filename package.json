{
  "name": "remix-provider",
  "version": "1.0.2",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js"
  },
  "description": "Provides the ability to set the content of the Remix head from within the component.",
  "license": "MIT",
  "scripts": {
    "build": "tsc && tsc --project ./tsconfig.esm.json && cpy esm dist",
    "test": "jest --coverage",
    "test:update": "jest -u --coverage",
    "lint": "eslint --fix ./src ./test",
    "lint:fix": "eslint --fix ./src"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@swc-node/jest": "^1.8.12",
    "@swc/core": "^1.11.8",
    "@swc/helpers": "^0.5.15",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.9",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "cpy-cli": "^5.0.0",
    "eslint": "9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0"
  },
  "keywords": [
    "remix",
    "cloudflare",
    "env",
    "provider"
  ],
  "author": "SoraKumo <info@croud.jp>",
  "repository": "https://github.com/ReactLibraries/remix-provider"
}
